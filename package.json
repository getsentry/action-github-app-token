{
  "name": "action-github-app-token",
  "version": "1.0.0",
  "description": "Gets a GitHub auth token for a GitHub App installation",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc && ncc build",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "all": "yarn format && yarn lint && yarn test && yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:getsentry/action-github-app-token.git"
  },
  "keywords": [
    "actions",
    "github app",
    "github auth",
    "github token",
    "github action"
  ],
  "author": "Sentry",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.2.0",
    "@octokit/auth-app": "^2.4.14",
    "@octokit/rest": "^18.0.3"
  },
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "@types/node": "^12.7.12",
    "@typescript-eslint/parser": "^2.8.0",
    "@zeit/ncc": "^0.20.5",
    "eslint": "^5.16.0",
    "eslint-plugin-github": "^2.0.0",
    "eslint-plugin-jest": "^22.21.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^24.9.0",
    "jest-circus": "^24.9.0",
    "js-yaml": "^3.13.1",
    "prettier": "^1.19.1",
    "ts-jest": "^24.2.0",
    "typescript": "^3.6.4"
  }
}
